source ~/.vimrc

set nocursorline
set winheight=10
set winwidth=10
set winminwidth=0
set winwidth=84
set laststatus=0
set noruler
set autoindent
set smartindent
set nocompatible
set colorcolumn=999

nmap <Leader>b <Plug>VimwikiGoBackLink

hi MatchParen ctermfg=0

augroup CursorLine
	autocmd!
augroup END

:cd /Users/Andre/Air/Wiki

let g:vimwiki_list = [{'path': '~/Air/Wiki', 'ext': '.md'}]
